{
    "manifest_version": 2,
    "name": "Skynight",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "https://www.facebook.com/*"
          ],
          "js": ["content.js"]
        }
      ],
      "background": {
        "scripts": ["background.js"]
      },
      "browser_action": {
        "default_icon": "icon.png"
      },
      "permissions": [
        "tabs",
        "activeTab"
      ]
  }